{
  "customerAdd": {
    "name": "Add new Customer",
    "fields": [
      {
        "controlType": "input",
        "label": "Name",
        "key": "name",
        "validators": [
          {
            "name": "required"
          },
          {
            "name": "pattern",
            "args": {
              "regexp": "^[a-zA-Z0-9 ]{3,}$"
            }
          }
        ],
        "errorMessage": "Min. 3 chars"
      },
      {
        "controlType": "input",
        "label": "Email",
        "key": "email",
        "validators": [
          {
            "name": "required"
          },
          {
            "name": "email"
          }
        ],
        "errorMessage": "Not a valid email"
      },
      {
        "controlType": "input",
        "label": "Address",
        "key": "address",
        "validators": [
          {
            "name": "required"
          }
        ],
        "errorMessage": "Required"
      },
      {
        "controlType": "input",
        "label": "IP address",
        "key": "ip_address",
        "validators": [
          {
            "name": "required"
          },
          {
            "name": "pattern",
